{
    "name": "sellmate/laravel-multi-tenant",
    "version": "12.0.0",
    "license": "MIT",
    "description": "Multi tenant package for Laravel Framework",
    "authors": [
        {
            "name": "Eunjun Park",
            "email": "ej@sellmate.co.kr"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/passport": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Sellmate\\Laravel\\MultiTenant\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sellmate\\Laravel\\MultiTenant\\Providers\\MultiTenantServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}