{
    "name": "sellmate/laravel-multi-tenant",
    "version" : "0.1.0",
    "license": "MIT",
    "description": "Multi tenant package for Laravel Framework",
    "authors": [
        {
            "name": "Eunjun Park",
            "email": "ej@sellmate.co.kr"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/passport": "^10.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3",
        "mockery/mockery": "^1.4.2",
        "orchestra/testbench": "^6.17"
    },
    "autoload": {
        "psr-4": {
            "Sellmate\\Laravel\\MultiTenant\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sellmate\\Laravel\\MultiTenant\\Providers\\MultiTenantServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
